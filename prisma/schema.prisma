generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum applicationType {
  Drift
  Utvikling
}

model application {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  title String
  url String
  expires DateTime
  positions Int
  type applicationType
  archivedText String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}